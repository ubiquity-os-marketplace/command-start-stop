{
  "name": "@ubiquity-os/command-start-stop",
  "version": "1.0.0",
  "description": "Enables the assignment and graceful unassignment of tasks to contributors.",
  "main": "src/worker.ts",
  "author": "Ubiquity DAO",
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "format": "run-s format:lint format:prettier format:cspell",
    "format:lint": "eslint --fix .",
    "format:prettier": "prettier --write .",
    "format:cspell": "tsx .github/cspell.ts",
    "knip": "knip --config .github/knip.ts",
    "knip-ci": "knip --no-exit-code --reporter json --config .github/knip.ts",
    "prepare": "husky install",
    "test": "jest --setupFiles dotenv/config --coverage",
    "worker": "wrangler dev --env dev --port 4000"
  },
  "keywords": [
    "typescript",
    "template",
    "dao",
    "ubiquity",
    "open-source"
  ],
  "dependencies": {
    "@octokit/graphql-schema": "15.25.0",
    "@octokit/plugin-rest-endpoint-methods": "^13.2.6",
    "@octokit/types": "^13.6.1",
    "@sinclair/typebox": "^0.32.0",
    "@supabase/supabase-js": "2.42.0",
    "@ubiquity-os/plugin-sdk": "^1.0.11",
    "@ubiquity-os/ubiquity-os-logger": "^1.3.2",
    "dotenv": "^16.4.4",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@cspell/dict-node": "^5.0.5",
    "@cspell/dict-software-terms": "^4.1.15",
    "@cspell/dict-typescript": "^3.1.2",
    "@eslint/js": "9.14.0",
    "@jest/globals": "29.7.0",
    "@mswjs/data": "0.16.1",
    "@types/jest": "29.5.12",
    "@types/ms": "^0.7.34",
    "@types/node": "20.14.5",
    "@octokit/rest": "20.1.1",
    "cspell": "8.9.0",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-check-file": "2.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-sonarjs": "1.0.3",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-md-dashboard": "0.8.0",
    "knip": "5.21.2",
    "lint-staged": "15.2.7",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.2",
    "ts-jest": "29.1.5",
    "ts-node": "^10.9.2",
    "tsx": "4.15.6",
    "typescript": "5.6.2",
    "typescript-eslint": "8.14.0",
    "wrangler": "^3.87.0"
  },
  "lint-staged": {
    "*.ts": [
      "yarn prettier --write",
      "eslint --fix"
    ],
    "src/**.{ts,json}": [
      "cspell"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "yarn@4.2.2"
}
